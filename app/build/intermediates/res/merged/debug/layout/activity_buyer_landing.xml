<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="com.example.akant.grabhousesecure.BuyerLandingActivity">

    <include
        android:id="@+id/app_bar"
        layout="@layout/bar" />

    <LinearLayout
        android:id="@+id/row1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/app_bar"
        android:layout_marginTop="20dp">

        <LinearLayout
            android:id="@+id/property"
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:layout_weight="1"
            android:layout_margin="20dp"
            android:orientation="vertical"
            android:weightSum="1"
            android:background="@drawable/hollow_btn_white"
            android:clickable="true">

            <ImageView
                android:id="@+id/property_image"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:src="@drawable/property"
                android:padding="20dp" />

            <TextView
                android:id="@+id/property_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#FFFFFF"
                android:text="@string/myproperty"
                android:background="#13234b"
                android:gravity="center_vertical|center_horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="1dp"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"
                android:singleLine="true"
                android:textAppearance="?android:textAppearanceMedium" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/key"
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:layout_weight="1"
            android:layout_margin="20dp"
            android:orientation="vertical"
            android:weightSum="1"
            android:background="@drawable/hollow_btn_white"
            android:clickable="true">

            <ImageView
                android:id="@+id/key_image"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:src="@drawable/keys"
                android:padding="20dp" />

            <TextView
                android:id="@+id/key_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="#FFFFFF"
                android:text="@string/Keys"
                android:background="#13234b"
                android:gravity="center_vertical|center_horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="1dp"
                android:layout_marginBottom="1dp"
                android:layout_marginRight="1dp"
                android:textAppearance="?android:textAppearanceMedium" />

        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
